{
  "name": "CalAcademy Skull Wall",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Custom css and js for Sketchfab embed",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://*.sketchfab.com/*",
        "https://*.sketchfab.com/*"
      ],
      "js": [
        "js/jquery-1.11.3.min.js",
        "js/inject.js"
      ],
      "all_frames": true
    }
  ]
}
